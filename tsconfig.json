{
  "compileOnSave": true,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "removeComments": false,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2016",
      "es2017",
      "dom"
    ]
  },
  "include": [
    "src/*",
    "typings/*"
  ],
    "exclude": [
      "node_modules",
      "**/*.spec.ts"
    ],
      "types": [
        "jquery"
      ],
      "noImplicitAny": true,
      "suppressImplicitAnyIndexErrors": true
}
